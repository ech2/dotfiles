! vim: set foldmarker={{,}} foldlevel=0 foldmethod=marker:
! Font rendering {{
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
! }}
! Colors {{
!#define DARK
#ifdef DARK
  #include "/home/ech/.config/ech/base16-grayscale-dark-256.Xresources"
#else
  #include "/home/ech/.config/ech/base16-grayscale-light-256.Xresources"
#endif
base0: base00
base1: base01
base2: base02
base3: base03
base4: base04
base5: base05
base6: base06
base7: base07
base8: base08
base9: base09
baseA: base10
baseB: base11
baseC: base12
baseD: base13
baseE: base14
baseF: base15
!}}
! rofi {{
rofi.lines: 5
rofi.width: 40
rofi.color-normal: base00, base05, base01, base02, base05
rofi.color-window: base00, base02, base02
! }}
! st {{
st.font: DejaVuSansMono:pixelsize=14:antialias=true:autohint=true
st.bold_font: 1
st.alpha: 255
! }}
