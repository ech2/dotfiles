! vim: set foldmarker={{,}} foldlevel=0 foldmethod=marker:
! Font rendering {{
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
! }}
! Colors {{
!#define DARK
!#ifdef DARK
!  #include "/home/ech/.config/ech/base16-grayscale-dark-256.Xresources"
!#else
!  #include "/home/ech/.config/ech/base16-grayscale-light-256.Xresources"
!#endif

*.foreground:   #98d1ce
*.background:   #0a0f14
*.cursorColor:  #98d1ce

*.color0:       #0a0f14
*.color8:       #10151b

*.color1:       #c33027
*.color9:       #d26939

*.color2:       #26a98b
*.color10:      #081f2d

*.color3:       #edb54b
*.color11:      #245361

*.color4:       #195465
*.color12:      #093748

*.color5:       #4e5165
*.color13:      #888ba5

*.color6:       #33859d
*.color14:      #599caa

*.color7:       #98d1ce
*.color15:      #d3ebe9
!}}
! rofi {{
rofi.lines: 5
rofi.width: 40
! }}
! st {{
st.font: DejaVuSansMono:pixelsize=14:antialias=true:autohint=true
st.bold_font: 1
st.alpha: 230
! }}
